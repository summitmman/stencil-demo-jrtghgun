{
  "name": "node-starter",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@stencil/core": "^2.5.2",
    "@types/jest": "26.0.23",
    "http-server": "^0.12.3",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "swiper": "6.5.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "stencil build --dev --watch --serve",
    "test:unit": "stencil test --spec --detectOpenHandles"
  },
  "jest": {
    "preset": "@stencil/core/testing",
    "modulePathIgnorePatterns": ["dist/.*", "www/.*", "hydrate/.*"],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "src/utils/*.{ts,tsx}"
    ]
  }
}
